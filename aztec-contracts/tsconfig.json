{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"outDir": "./dist",
		"rootDir": ".",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	// "include": ["src", "src/*.ts", "src/**/*.ts"],
	"include": ["src"]
}
